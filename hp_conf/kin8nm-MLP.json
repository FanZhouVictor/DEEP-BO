{
    "dataset": "kin8nm",
    "model": "MLP",
    "config": { 
        "max_epoch": 27 
    },
    "hyperparams": {
        "scaler": {
            "type": "str",
            "value_type": "categorical",
            "range": [
                "StandardScaler",
                "RobustScaler",
                "MinMaxScaler",
                "MaxAbsScaler"
            ]
        },        
        "n_layers": {
            "type": "int",
            "value_type": "discrete",
            "range": [1, 5]
        },
        "init": {
            "type": "str",
            "value_type": "categorical",
            "range": [
                "uniform",
                "normal",
                "glorot_uniform",
                "glorot_normal",
                "he_uniform",
                "he_normal"
            ]
        },           
        "batch_size": {
            "type": "int",
            "value_type": "preordered",
            "range": [16, 32, 64, 128, 256]
        },
        "shuffle": {
            "type": "bool",
            "value_type": "categorical",
            "range": [
                false,
                true
            ]
        },
        "loss": {
            "type": "str",
